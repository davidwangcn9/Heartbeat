{
  "name": "docs",
  "version": "0.0.7",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "test": "vitest",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "prettier --check .",
    "fix": "prettier --write . --ignore-unknown",
    "algolia": "tsm ./scripts/generateAndUploadToAlgolia.ts",
    "prepare": "cd .. && husky install docs/.husky",
    "format": "pnpm run format:code",
    "format:ci": "pnpm run format:imports && pnpm run format:code",
    "format:code": "prettier -w . --cache --plugin-search-dir=.",
    "format:imports": "organize-imports-cli ./tsconfig.json",
    "add-language": "node ./scripts/add-language.mjs",
    "docgen": "node ./scripts/docgen.mjs",
    "docgen:errors": "node ./scripts/error-docgen.mjs",
    "docgen:integrations": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/generate-integration-pages.ts",
    "lint:a11y": "start-test 'yarn build && yarn preview' 3000 'yarn lint:a11y:local'",
    "lint:a11y:local": "pa11y-ci --sitemap 'http://localhost:3000/sitemap.xml' --sitemap-find 'https://docs.astro.build' --sitemap-replace 'http://localhost:3000'",
    "lint:a11y:remote": "pa11y-ci --sitemap 'https://docs.astro.build/sitemap.xml'",
    "lint:linkcheck": "SKIP_OG=true pnpm build && tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:linkcheck:nobuild": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:slugcheck": "node ./scripts/lint-slugcheck.mjs",
    "lint:eslint": "eslint .",
    "netlify:build": "pnpm ${NETLIFY_BUILD_SCRIPT:-build}",
    "translation-status": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/translation-status.ts"
  },
  "devDependencies": {
    "@11ty/eleventy-fetch": "^4.0.1",
    "@actions/core": "^1.10.1",
    "@astrojs/mdx": "^3.1.2",
    "@astrojs/preact": "^3.5.0",
    "@astrojs/sitemap": "3.1.6",
    "@babel/core": "^7.24.7",
    "@docsearch/css": "^3.6.0",
    "@docsearch/react": "^3.6.0",
    "@types/canvas-confetti": "^1.6.4",
    "@types/hast": "^3.0.4",
    "@types/html-escaper": "^3.0.2",
    "@types/mdast": "^4.0.4",
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "algoliasearch": "^4.24.0",
    "astro": "^4.11.3",
    "astro-auto-import": "^0.4.2",
    "astro-eslint-parser": "^1.0.2",
    "astro-expressive-code": "^0.35.3",
    "astro-og-canvas": "^0.5.1",
    "bcp-47-normalize": "^2.3.0",
    "canvaskit-wasm": "^0.39.1",
    "dedent-js": "^1.0.1",
    "domhandler": "^5.0.3",
    "eslint": "^9.5.0",
    "eslint-plugin-astro": "^1.2.2",
    "eslint-plugin-react": "^7.34.3",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "hast-util-from-html": "^2.0.1",
    "hast-util-to-html": "^9.0.1",
    "hast-util-to-string": "^3.0.0",
    "hastscript": "^9.0.0",
    "html-escaper": "^3.0.3",
    "htmlparser2": "^9.1.0",
    "husky": "^9.0.11",
    "kleur": "^4.1.5",
    "lint-staged": "^15.2.7",
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-mdx-jsx": "^3.1.2",
    "mdast-util-to-hast": "^13.2.0",
    "mdast-util-to-string": "^4.0.0",
    "micromark-util-character": "^2.1.0",
    "micromark-util-symbol": "^2.0.0",
    "node-fetch": "^3.3.2",
    "organize-imports-cli": "^0.10.0",
    "p-retry": "^6.2.0",
    "parse-numeric-range": "^1.3.0",
    "preact": "^10.22.0",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0",
    "prompts": "^2.4.2",
    "rehype": "^13.0.1",
    "remark": "^15.0.1",
    "remark-directive": "^3.0.0",
    "remove-markdown": "^0.5.0",
    "simple-git": "^3.25.0",
    "tsm": "^2.3.0",
    "typescript": "^5.5.2",
    "unified": "^11.0.5",
    "unist-util-remove": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-walker": "^1.0.0",
    "vfile": "^6.0.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@akebifiky/remark-simple-plantuml": "^1.0.2",
    "@astrojs/check": "^0.7.0",
    "@fontsource/ibm-plex-mono": "5.0.13",
    "@nanostores/preact": "^0.5.1",
    "canvas-confetti": "^1.9.3",
    "jsdoc-api": "^8.0.0",
    "minimatch": "^9.0.5",
    "nanostores": "^0.10.3",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-smartypants": "^3.0.1",
    "sass": "^1.77.6"
  },
  "engines": {
    "node": ">=18.14.1"
  },
  "lint-staged": {
    "**/*": "npm run fix"
  },
  "packageManager": "pnpm@9.0.6",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "vite"
      ]
    }
  }
}
